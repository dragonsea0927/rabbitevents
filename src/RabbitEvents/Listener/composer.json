{
  "name": "rabbitevents/listener",
  "description": "The Listener part of the RabbitEvents package.",
  "keywords": ["laravel", "rabbitmq", "events", "listener", "sub"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Kirdzei",
      "email": "masterjus@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "illuminate/console": "^8.0|^9.0",
    "illuminate/events": "^8.0|^9.0",
    "illuminate/support": "^8.0|^9.0",
    "enqueue/amqp-lib": "^0.10",
    "rabbitevents/foundation": "7.x-dev"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "RabbitEvents\\Listener\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ListenerServiceProvider"
      ],
      "aliases": {
        "RabbitEvents": "RabbitEvents\\Listener\\Facades\\RabbitEvents"
      }
    }
  },
  "suggest": {
    "ext-pcntl": "Required to use all features of the worker."
  },
  "autoload-dev": {
    "psr-4": {
      "RabbitEvents\\Listener\\Tests\\": "tests/"
    }
  }
}
